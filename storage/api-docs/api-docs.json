                ],
                "summary": "Create a new enter",
                "operationId": "7fedfb679f8377dba5e0dd115e9b356d",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "first_name": {
                                        "type": "string",
                                        "example": "example_value"
                                    },
                                    "last_name": {
                                        "type": "string",
                                        "example": "example_value"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
